{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "isolatedModules": true,
    "noEmitOnError": true,
    "noImplicitAny": false
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
